    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-sort-class-members": "1.6.0",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "rxjs-marbles": "5.0.3",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
