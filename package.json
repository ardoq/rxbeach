{
  "name": "rxbeach",
  "version": "0.0.1",
  "description": "Chill with streams at the RxBeach",
  "keywords": ["rxjs", "reactive", "streams", "beach", "rx"],
  "company": "Ardoq AS",
  "license": "MIT",
  "scripts": {
    "test": "ts-mocha --paths -p tsconfig.json src/*.tests.ts src/**/*.tests.ts examples/**/*.tests.ts",
    "check-types": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts src examples",
    "build": "rimraf dist && yarn tsc -p tsconfig-build.json",
    "publish:prep": "yarn build && node prep-package.js",
    "publish": "yarn publish:prep && cd ./dist/ && yarn publish",
    "pack": "yarn publish:prep && cd ./dist/ && npm pack"
  },
  "author": "Tobias Laundal",
  "devDependencies": {
    "@testing-library/react-hooks": "2.0.1",
    "@types/assert": "1.4.3",
    "@types/mocha": "5.2.7",
    "@types/react": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "assert": "2.0.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-sort-class-members": "1.6.0",
    "mocha": "6.2.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0",
    "rimraf": "3.0.0",
    "rxjs-marbles": "5.0.3",
    "rxjs-subscription-count": "1.0.1",
    "ts-mocha": "6.0.0",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.6.2"
  },
  "dependencies": {
    "react": "16.9.0",
    "rxjs": "6.5.3",
    "rxjs-spy": "7.5.1"
  }
}
